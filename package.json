{
  "name": "alpha-beta-component",
  "version": "0.1.0",
  "description": "Simple declarative A/B testing component for React.",
  "scripts": {
    "lint": "`npm bin`/eslint --ext .js,.jsx .",
    "test": "`npm bin`/mocha --compilers js:babel-register --recursive test/*.js --reporter spec --ui bdd",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [
    "A/B",
    "split",
    "testing",
    "declarative",
    "component",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-alpha-beta/alphabeta.git"
  },
  "bugs": {
    "url": "https://github.com/react-alpha-beta/alphabeta/issues"
  },
  "authors": [
    "Ben Hall <benjaminxhall@gmail.com> (https://github.com/bxh-io)",
    "Jack McCloy <mccloy@me.com> (https://github.com/jackmccloy)",
    "Brian Park <yaru22@gmail.com> (https://github.com/yaru22)"
  ],
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^5.0.0",
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.1.5",
    "enzyme": "^2.1.0",
    "eslint": "^2.4.0",
    "eslint-plugin-react": "^4.2.3",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
